@spec Should handle typical api docs
  @versions
    0.1.0
    0.2.0
    0.3.0
  @source
    @filename filename.um
    @content
      @module
        @version 0.1.0
          @api
            @prototype Bob
              @constructor
                @description: Does something
                @param a [Number]: The a parameter
        @version 0.2.0
          @api
            @prototype Bob
              @constructor
                @description: Does something better
                @param a [Number]: The a parameter

              @constructor
                @description: Does another thing
                @param a [Number]: The a parameter
                @param b [Number]: The b parameter

  @expected
    @output
      @filename 0.1.0/filename.um
      @content
        @module
          @api
            @prototype Bob
              @constructor
                @description: Does something
                @param a [Number]: The a parameter

    @output
      @filename 0.2.0/filename.um
      @content
        @module
          @api
            @prototype Bob
              @constructor
                @description: Does something better
                @param a [Number]: The a parameter
                @updated
              @constructor
                @description: Does another thing
                @param a [Number]: The a parameter
                @param b [Number]: The b parameter
                @added
              @updated

    @output
      @filename 0.3.0/filename.um
      @content
        @module
          @api
            @prototype Bob
              @constructor
                @description: Does something better
                @param a [Number]: The a parameter
              @constructor
                @description: Does another thing
                @param a [Number]: The a parameter
                @param b [Number]: The b parameter
